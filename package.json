{
  "name": "prepublish-lib",
  "version": "0.3.8",
  "description": "A solution for simplifying pre-publishing typical JavaScript projects for publishing to NPM.",
  "keywords": [
    "npm",
    "publish",
    "library"
  ],
  "engines": {
    "node": "~6.3.0"
  },
  "bin": {
    "prepublish-lib": "./bin/prepublish-lib.js"
  },
  "author": "Sebastian Werner",
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "lint": "eslint --quiet src/ test/ || true",
    "nsp": "nsp check --output summary",
    "release": "release-it --github.release --npm.publish --non-interactive",
    "release:minor": "release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "release-it --github.release --npm.publish --non-interactive --increment major",
    "test": "rimraf test/lib test/dist && npm run prepublish && node bin/prepublish-lib test/src/index.js test/lib && webpack --config test/webpack.config.js && node test/dist/bundle.js",
    "prepublish": "rimraf bin && rollup -c rollup.cfg"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastian-software/prepublish-lib.git"
  },
  "dependencies": {
    "async": "^2.0.1",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-es2017": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "babel-preset-stage-3": "^6.11.0",
    "babel-runtime": "^6.11.6",
    "builtin-modules": "^1.1.1",
    "denodeify": "^1.2.1",
    "fs-extra": "^0.30.0",
    "loader-utils": "^0.2.15",
    "lodash": "^4.15.0",
    "read-package-json": "^2.0.4",
    "rollup": "^0.34.13",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-buble": "^0.13.0",
    "rollup-plugin-json": "^2.0.2",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-relink": "^0.1.7"
  },
  "devDependencies": {
    "file-loader": "^0.9.0",
    "rimraf": "^2.5.4",
    "s15e-javascript": "^0.2.3",
    "webpack": "^2.1.0-beta.22"
  }
}
